{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { postData } from '../service/apiService'; // Assurez-vous que le chemin est correct\n\nexport default {\n  name: 'LoginConnexion',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async handleLogin() {\n      postData('/login', {\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        console.log(response);\n        if (response.status === 200) {\n          localStorage.setItem('user', this.email);\n          this.$router.push('/dashboard');\n        } else {\n          // Lire le message d'erreur de la réponse\n          window.alert(`Erreur ${response.status} : ${response.data.message || 'Erreur inconnue'}`);\n        }\n      }).catch(error => {\n        if (error.response) {\n          window.alert(`Erreur ${error.response.status} : ${error.response.data || 'Erreur inconnue'}`);\n        } else {\n          window.alert(`Une erreur est survenue : ${error.message || 'Erreur inconnue'}`);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["postData","name","data","email","password","methods","handleLogin","then","response","console","log","status","localStorage","setItem","$router","push","window","alert","message","catch","error"],"sources":["/Users/stagiaire_mobilosoft/Documents/Clean_Code_Mobilo/src/views/Login.vue"],"sourcesContent":["<template>\n    <div class=\"container wrapper p-5\">\n        <div class=\"wrapper card py-5\" style=\"width:600px;\">\n            <div class=\"card-body\">\n                <h1>Mobilosoft API DOCS Configuration</h1>\n                <h3>Connectez-vous en tant qu'admin.</h3>\n                <form @submit.prevent=\"handleLogin\">\n                    <label for=\"email\">\n                        Email :\n                    </label>\n                    <input v-model=\"email\" type=\"text\" id=\"email\" placeholder=\"Entrer votre Email\" required>\n\n                    <label for=\"password\">\n                        Mot de passe :\n                    </label>\n                    <input v-model=\"password\" type=\"password\" id=\"password\" placeholder=\"Entrer votre Mot de passe\" required>\n\n                    <div class=\"wrap\">\n                        <button type=\"submit\">\n                        Se connecter\n                        </button>\n                    </div>\n                </form>\n                <br>\n                <p>Pas encore inscrit ?\n                    <router-link to=\"/s-inscrire\" style=\"text-decoration: none;\">\n                        Créer votre compte\n                    </router-link>\n                </p>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { postData } from '../service/apiService'; // Assurez-vous que le chemin est correct\n\nexport default {\n  name: 'LoginConnexion',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async handleLogin() {\n      postData('/login', {\n        email: this.email,\n        password: this.password\n      })\n      .then(response => {\n        console.log(response);\n        \n        if (response.status === 200) {\n          localStorage.setItem('user', this.email);\n          this.$router.push('/dashboard');\n        } else {\n          // Lire le message d'erreur de la réponse\n          window.alert(`Erreur ${response.status} : ${response.data.message || 'Erreur inconnue'}`);\n        }\n      })\n      .catch(error => {\n        if (error.response) {\n          window.alert(`Erreur ${error.response.status} : ${error.response.data || 'Erreur inconnue'}`);\n        } else {\n          window.alert(`Une erreur est survenue : ${error.message || 'Erreur inconnue'}`);\n        }\n      });\n    }\n  }\n}\n</script>\n\n<style scoped>\n/* Style déjà fourni */\n</style>\n\n\n<style scoped>\n\n.wrapper {\n  display: flex;\n  align-items: center;\n  flex-direction: column; \n  justify-content: center;\n  width: 100%;\n  min-height: 100%;\n  padding: 20px;\n}\n\n.card {\n    box-shadow: 0px 0px 20px 2px #cacaca;\n    border-radius: 15px;\n}\n\n\nh1 {\n  color: #022D7E;\n  font-size: 30px;\n}\n\nlabel {\n  display: block;\n  width: 100%;\n  margin-top: 10px;\n  margin-bottom: 5px;\n  text-align: left;\n  color: #555;\n  font-weight: bold;\n}\n\ninput {\n  display: block;\n  width: 100%;\n  margin-bottom: 15px;\n  padding: 10px;\n  box-sizing: border-box;\n  border: 1px solid #ddd;\n  border-radius: 5px;\n}\n\n\nbutton {\n  padding: 15px;\n  border-radius: 10px;\n  margin-top: 20px;\n  margin-bottom: 15px;\n  border: none;\n  color: white;\n  cursor: pointer;\n  background-color: #022D7E;\n  width: 100%;\n  font-size: 16px;\n}\n</style>\n"],"mappings":";AAmCA,SAASA,QAAO,QAAS,uBAAuB,EAAE;;AAElD,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,WAAWA,CAAA,EAAG;MAClBN,QAAQ,CAAC,QAAQ,EAAE;QACjBG,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACAG,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QAErB,IAAIA,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAC3BC,YAAY,CAACC,OAAO,CAAC,MAAM,EAAE,IAAI,CAACV,KAAK,CAAC;UACxC,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC,YAAY,CAAC;QACjC,OAAO;UACL;UACAC,MAAM,CAACC,KAAK,CAAC,UAAUT,QAAQ,CAACG,MAAM,MAAMH,QAAQ,CAACN,IAAI,CAACgB,OAAM,IAAK,iBAAiB,EAAE,CAAC;QAC3F;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACd,IAAIA,KAAK,CAACZ,QAAQ,EAAE;UAClBQ,MAAM,CAACC,KAAK,CAAC,UAAUG,KAAK,CAACZ,QAAQ,CAACG,MAAM,MAAMS,KAAK,CAACZ,QAAQ,CAACN,IAAG,IAAK,iBAAiB,EAAE,CAAC;QAC/F,OAAO;UACLc,MAAM,CAACC,KAAK,CAAC,6BAA6BG,KAAK,CAACF,OAAM,IAAK,iBAAiB,EAAE,CAAC;QACjF;MACF,CAAC,CAAC;IACJ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}