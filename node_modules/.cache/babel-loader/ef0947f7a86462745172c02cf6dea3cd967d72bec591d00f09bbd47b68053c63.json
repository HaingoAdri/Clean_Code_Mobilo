{"ast":null,"code":"import { fetchData, putData, deleteData } from '@/service/apiService';\nimport * as bootstrap from 'bootstrap';\nexport default {\n  name: 'Header_vue',\n  data() {\n    return {\n      headers: [],\n      updatedHeader: {},\n      updateModalInstance: null\n    };\n  },\n  mounted() {\n    this.fetchHeaders();\n  },\n  methods: {\n    async fetchHeaders() {\n      try {\n        const response = await fetchData(`/header/allHeader`); // Assurez-vous que votre API a cette route\n        if (response) {\n          this.headers = response;\n        }\n      } catch (error) {\n        console.error('Erreur lors de la récupération des headers:', error);\n      }\n    },\n    openUpdateModal(header) {\n      this.updatedHeader = {\n        ...header\n      }; // Dupliquez l'objet pour le modifier\n      const modalElement = document.getElementById('updateModal');\n      this.updateModalInstance = new bootstrap.Modal(modalElement);\n      this.updateModalInstance.show();\n    },\n    async handleUpdate() {\n      try {\n        const response = await putData(`/header/update_header/${this.updatedHeader.id}`, this.updatedHeader);\n        if (response) {\n          alert('Header mis à jour avec succès !');\n          this.fetchHeaders();\n          this.updateModalInstance.hide();\n        }\n      } catch (error) {\n        console.error('Erreur lors de la mise à jour:', error);\n      }\n    },\n    async handleDelete(id) {\n      if (confirm('Voulez-vous vraiment supprimer ce header ?')) {\n        try {\n          await deleteData(`/header/delete_header/${id}`);\n          alert('Header supprimé avec succès !');\n          this.fetchHeaders();\n        } catch (error) {\n          console.error('Erreur lors de la suppression:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["fetchData","putData","deleteData","bootstrap","name","data","headers","updatedHeader","updateModalInstance","mounted","fetchHeaders","methods","response","error","console","openUpdateModal","header","modalElement","document","getElementById","Modal","show","handleUpdate","id","alert","hide","handleDelete","confirm"],"sources":["/Users/eva_smartelia/Documents/Clean_Code_Mobilo/src/components/endpoints_menu/reponse/Header_reponse.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"card mb-4\">\n            <div class=\"card-header rounded-bottom rounded-4\">\n                <h3 class=\"py-2\">Liste des Headers</h3>\n            </div>\n            <div class=\"card-body\">\n                <blockquote class=\"blockquote mb-0\">\n                    <table class=\"table table-bordered\">\n                        <thead>\n                            <tr>\n                                <th>Nom</th>\n                                <th>Types</th>\n                                <th>Détails</th>\n                                <th>Sous-catégorie</th>\n                                <th>Action</th>\n                            </tr>\n                        </thead>\n                        <tbody>\n                            <tr v-for=\"header in headers\" :key=\"header.id\">\n                                <td>{{ header.nom }}</td>\n                                <td>{{ header.types }}</td>\n                                <td>{{ header.details }}</td>\n                                <td>{{ header.sous_categorie }}</td>\n                                <td>\n                                    <button class=\"btn m-2\" @click=\"openUpdateModal(header)\"><i class=\"bi bi-pen-fill\"></i></button>\n                                    <button class=\"btn\" @click=\"handleDelete(header.id)\"><i class=\"bi bi-file-earmark-x-fill\"></i></button>\n                                </td>\n                            </tr>\n                        </tbody>\n                    </table>\n                </blockquote>\n            </div>\n        </div>\n\n        <!-- Modal pour mettre à jour les données -->\n        <div class=\"modal fade\" id=\"updateModal\" tabindex=\"-1\" aria-labelledby=\"updateModalLabel\" aria-hidden=\"true\">\n            <div class=\"modal-dialog\">\n                <div class=\"modal-content\">\n                    <div class=\"modal-header\">\n                        <h5 class=\"modal-title\" id=\"updateModalLabel\">Mise à jour du Header</h5>\n                        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n                    </div>\n                    <div class=\"modal-body\">\n                        <form @submit.prevent=\"handleUpdate\">\n                            <div class=\"mb-3\">\n                                <label class=\"form-label\">Nom :</label>\n                                <input type=\"text\" class=\"form-control\" v-model=\"updatedHeader.nom\" />\n                            </div>\n                            <div class=\"mb-3\">\n                                <label class=\"form-label\">Types :</label>\n                                <input type=\"text\" class=\"form-control\" v-model=\"updatedHeader.types\" />\n                            </div>\n                            <div class=\"mb-3\">\n                                <label class=\"form-label\">Détails :</label>\n                                <textarea class=\"form-control\" v-model=\"updatedHeader.details\"></textarea>\n                            </div>\n                            <div class=\"mb-3\">\n                                <label class=\"form-label\">Sous-catégorie :</label>\n                                <input type=\"number\" class=\"form-control\" v-model=\"updatedHeader.sous_categorie\" />\n                            </div>\n                            <button type=\"submit\" class=\"btn btn-primary\">Mettre à jour</button>\n                        </form>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { fetchData, putData, deleteData } from '@/service/apiService';\nimport * as bootstrap from 'bootstrap';\n\nexport default {\n    name: 'Header_vue',\n    data() {\n        return {\n            headers: [],\n            updatedHeader: {},\n            updateModalInstance: null,\n        };\n    },\n    mounted() {\n        this.fetchHeaders();\n    },\n    methods: {\n        async fetchHeaders() {\n            try {\n                const response = await fetchData(`/header/allHeader`); // Assurez-vous que votre API a cette route\n                if (response) {\n                    this.headers = response;\n                }\n            } catch (error) {\n                console.error('Erreur lors de la récupération des headers:', error);\n            }\n        },\n\n        openUpdateModal(header) {\n            this.updatedHeader = { ...header }; // Dupliquez l'objet pour le modifier\n            const modalElement = document.getElementById('updateModal');\n            this.updateModalInstance = new bootstrap.Modal(modalElement);\n            this.updateModalInstance.show();\n        },\n\n        async handleUpdate() {\n            try {\n                const response = await putData(`/header/update_header/${this.updatedHeader.id}`, this.updatedHeader);\n                if (response) {\n                    alert('Header mis à jour avec succès !');\n                    this.fetchHeaders();\n                    this.updateModalInstance.hide();\n                }\n            } catch (error) {\n                console.error('Erreur lors de la mise à jour:', error);\n            }\n        },\n\n        async handleDelete(id) {\n            if (confirm('Voulez-vous vraiment supprimer ce header ?')) {\n                try {\n                    await deleteData(`/header/delete_header/${id}`);\n                    alert('Header supprimé avec succès !');\n                    this.fetchHeaders();\n                } catch (error) {\n                    console.error('Erreur lors de la suppression:', error);\n                }\n            }\n        }\n    }\n}\n</script>\n\n<style scoped>\n.card {\n    border-radius: 15px;\n}\n.card-header {\n    background-color: #022D7E;\n    color: white;\n}\nbutton {\n    background-color: #28aabb;\n    color: white;\n}\n</style>\n"],"mappings":"AAuEA,SAASA,SAAS,EAAEC,OAAO,EAAEC,UAAS,QAAS,sBAAsB;AACrE,OAAO,KAAKC,SAAQ,MAAO,WAAW;AAEtC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,CAAC,CAAC;MACjBC,mBAAmB,EAAE;IACzB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,YAAY,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,YAAYA,CAAA,EAAG;MACjB,IAAI;QACA,MAAME,QAAO,GAAI,MAAMZ,SAAS,CAAC,mBAAmB,CAAC,EAAE;QACvD,IAAIY,QAAQ,EAAE;UACV,IAAI,CAACN,OAAM,GAAIM,QAAQ;QAC3B;MACJ,EAAE,OAAOC,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,6CAA6C,EAAEA,KAAK,CAAC;MACvE;IACJ,CAAC;IAEDE,eAAeA,CAACC,MAAM,EAAE;MACpB,IAAI,CAACT,aAAY,GAAI;QAAE,GAAGS;MAAO,CAAC,EAAE;MACpC,MAAMC,YAAW,GAAIC,QAAQ,CAACC,cAAc,CAAC,aAAa,CAAC;MAC3D,IAAI,CAACX,mBAAkB,GAAI,IAAIL,SAAS,CAACiB,KAAK,CAACH,YAAY,CAAC;MAC5D,IAAI,CAACT,mBAAmB,CAACa,IAAI,CAAC,CAAC;IACnC,CAAC;IAED,MAAMC,YAAYA,CAAA,EAAG;MACjB,IAAI;QACA,MAAMV,QAAO,GAAI,MAAMX,OAAO,CAAC,yBAAyB,IAAI,CAACM,aAAa,CAACgB,EAAE,EAAE,EAAE,IAAI,CAAChB,aAAa,CAAC;QACpG,IAAIK,QAAQ,EAAE;UACVY,KAAK,CAAC,iCAAiC,CAAC;UACxC,IAAI,CAACd,YAAY,CAAC,CAAC;UACnB,IAAI,CAACF,mBAAmB,CAACiB,IAAI,CAAC,CAAC;QACnC;MACJ,EAAE,OAAOZ,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;MAC1D;IACJ,CAAC;IAED,MAAMa,YAAYA,CAACH,EAAE,EAAE;MACnB,IAAII,OAAO,CAAC,4CAA4C,CAAC,EAAE;QACvD,IAAI;UACA,MAAMzB,UAAU,CAAC,yBAAyBqB,EAAE,EAAE,CAAC;UAC/CC,KAAK,CAAC,+BAA+B,CAAC;UACtC,IAAI,CAACd,YAAY,CAAC,CAAC;QACvB,EAAE,OAAOG,KAAK,EAAE;UACZC,OAAO,CAACD,KAAK,CAAC,gCAAgC,EAAEA,KAAK,CAAC;QAC1D;MACJ;IACJ;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}